.unselectable
  .col-md-12
    .widget-container.fluid-height.stf-device-list-tabs
      .widget-content.padded
        uib-tabset.overflow-auto
          uib-tab(active='activeTabs.setting')
            uib-tab-heading
              i.fa.fa-lg.color-blue
              span 功能设置
            div(ng-if='activeTabs.setting')
              unfollow-setting

          uib-tab(active='activeTabs.whitelist')
            uib-tab-heading
              i.fa.fa-lg.color-blue
              span 白名单
            div(ng-if='activeTabs.whitelist')
              white-list-setting

          uib-tab(active='activeTabs.reslist')
            uib-tab-heading
              i.fa.fa-lg.color-blue
              span 资源库
            div(ng-if='activeTabs.reslist')
              res-list-setting

          uib-tab(active='activeTabs.logs')
            uib-tab-heading
              i.fa.fa-lg.color-blue
              span 日志
            div(ng-if='activeTabs.logs')
              func-logs(type='2')
