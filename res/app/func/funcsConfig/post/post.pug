.unselectable
  .col-md-12
    .widget-container.fluid-height.stf-device-list-tabs
      .widget-content.padded
        uib-tabset.overflow-auto
          uib-tab(active='activeTabs.setting')
            uib-tab-heading
              i.fa.fa-lg.color-blue
              span 立即发帖
            div(ng-if='activeTabs.nowPost')
              unfollow-setting

          uib-tab(active='activeTabs.setting')
            uib-tab-heading
              i.fa.fa-lg.color-blue
              span 定时发帖
            div 定时发帖

          uib-tab(active='activeTabs.setting')
            uib-tab-heading
              i.fa.fa-lg.color-blue
              span 随机发帖
            div 随机发帖

          uib-tab(active='activeTabs.logs')
            uib-tab-heading
              i.fa.fa-lg.color-blue
              span 日志
            div(ng-if='activeTabs.logs')
              func-logs(type='5')
